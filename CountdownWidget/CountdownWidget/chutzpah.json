{
  "Framework": "jasmine",
  "TestFileTimeout": 60000,
  "TestHarnessReferenceMode": "AMD",
  "TypeScriptModuleKind": "AMD",
  "Compile": {
    "Mode": "External",
    "Extensions": [ ".ts" ],
    "ExtensionsWithNoOutput": [ ".d.ts" ],
    "UseSourceMaps": true
  },
  "References": [
    { "Path": "./scripts/lib/moment.js" },
    { "Path": "./scripts/lib/moment-timezone.js" },
    { "Path": "./scripts/lib/jquery.js" },
    { "Path": "./scripts/lib/VSS.SDK.js" },
    { "Path": "./node_modules/requirejs/require.js" }
  ],
  "Tests": [
    {
      "Includes": [ "*/tests/*.ts" ],
      "Excludes": [ "*.d.ts", "*/scripts/*/*.ts", "*/tests/TestSpec.ts" ]
    }

  ],
  "CodeCoverageExecutionMode": "Always",
  "CodeCoverageIncludes": [ "./scripts/countdownCalculator.ts" ],
  "CodeCoverageExcludes": [ "*/moment-timezone.js", "*/moment.js" ],
  "CodeCoverageTimeout": 500000,
  "Transforms": [

    {
      "Name": "jacoco",
      "Path": "jacoco.xml"

    }
  ]





}